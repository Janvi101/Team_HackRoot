{
    "version": 2,
    "name": "krishi-route",
    "rewrites": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/server.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/build/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/build/$1",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/build/index.html"
        }
    ]
}